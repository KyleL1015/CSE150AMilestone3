        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "df = pd.read_csv('weather_data.csv')"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def discretize_precipitation(p):\n",
        "    if p == 0: return 'none'\n",
        "    elif p <= 5: return 'light'\n",
        "    else: return 'heavy'\n",
        "\n",
        "def discretize_temp(t):\n",
        "    if t <= 0: return 'freezing'\n",
        "    elif t <= 5: return 'cold'\n",
        "    elif t <= 15: return 'cool'\n",
        "    else: return 'warm'\n",
        "\n",
        "def discretize_wind(w):\n",
        "    if w <= 2: return 'calm'\n",
        "    elif w <= 5: return 'moderate'\n",
        "    else: return 'strong'\n",
        "\n",
        "df['precip_cat'] = df['precipitation'].apply(discretize_precipitation)\n",
        "df['temp_max_cat'] = df['temp_max'].apply(discretize_precipitation)\n",
        "df['temp_min_cat'] = df['temp_min'].apply(discretize_precipitation)\n",
        "df['wind_cat'] = df['wind'].apply(discretize_wind)"
      ],
